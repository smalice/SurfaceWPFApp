			<DoubleAnimationUsingKeyFrames 
                Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[0].(GradientStop.Offset)" 
                Storyboard.TargetName="card">
				<SplineDoubleKeyFrame 
                    KeyTime="0" 
                    Value="0.087999999999999967"/>
			</DoubleAnimationUsingKeyFrames>
			<ColorAnimationUsingKeyFrames 
                Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" 
                Storyboard.TargetName="card">
				<SplineColorKeyFrame 
                    KeyTime="0" 
                    Value="White"/>
				<SplineColorKeyFrame 
                    KeyTime="0:0:0.3" 
                    Value="Gray"/>
			</ColorAnimationUsingKeyFrames>
			<ColorAnimationUsingKeyFrames 
                Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[1].(GradientStop.Color)" 
                Storyboard.TargetName="card">
				<SplineColorKeyFrame 
                    KeyTime="0" 
                    Value="Black"/>
				<SplineColorKeyFrame 
                    KeyTime="0:0:0.3" 
                    Value="Black"/>
			</ColorAnimationUsingKeyFrames>
			<ColorAnimationUsingKeyFrames 
                Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[0].(GradientStop.Color)" 
                Storyboard.TargetName="card">
				<SplineColorKeyFrame 
                    KeyTime="0" 
                    Value="SteelBlue"/>
				<SplineColorKeyFrame 
                    KeyTime="0:0:0.3" 
                    Value="#FF244662"/>
			</ColorAnimationUsingKeyFrames>
		</Storyboard>
		<Storyboard 
            x:Key="FadeInCard">
			<ColorAnimationUsingKeyFrames 
                Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" 
                Storyboard.TargetName="card">
				<SplineColorKeyFrame 
                    KeyTime="0" 
                    Value="Gray"/>
				<SplineColorKeyFrame 
                    KeyTime="0:0:0.3" 
                    Value="White"/>
			</ColorAnimationUsingKeyFrames>
			<ColorAnimationUsingKeyFrames 
                Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[0].(GradientStop.Color)" 
                Storyboard.TargetName="card">
				<SplineColorKeyFrame 
                    KeyTime="0" 
                    Value="#FF244662"/>
				<SplineColorKeyFrame 
                    KeyTime="0:0:0.3" 
                    Value="SteelBlue"/>
			</ColorAnimationUsingKeyFrames>
			<ColorAnimationUsingKeyFrames 
                Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[1].(GradientStop.Color)" 
                Storyboard.TargetName="card">
				<SplineColorKeyFrame 
                    KeyTime="0" 
                    Value="Black"/>
				<SplineColorKeyFrame 
                    KeyTime="0:0:0.3" 
                    Value="Black"/>
			</ColorAnimationUsingKeyFrames>
		</Storyboard>
	</s:SurfaceUserControl.Resources>
